export ORACLE_HOME=/oradbhome/dbhome_1
export PATH=$ORACLE_HOME/OPatch:$PATH
export LD_LIBRARY_PATH=$ORACLE_HOME/lib:/lib:/usr/lib/


Profile & User Creation

select username, account_status, default_tablespace from dba_users;
select PROPERTY_NAME, PROPERTY_VALUE from database_properties where PROPERTY_NAME like '%DEFAULT%';



ssh -X oracle@192.168.186.201
export CV_ASSUME_DISTID=OL8
export CV_ASSUME_DISTID=OEL7.8
export ORACLE_SID=TESTDB
export ORACLE_BASE=/oradbhome
