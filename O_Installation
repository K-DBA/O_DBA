./runInstaller -applyRU /oradbhome/dbhome_1/patches/36916690

cd database
./runInstaller -applyRU /patches/RU1925/36912597 \
  -silent \
  -responseFile ./install/response/db_install.rsp \
  oracle.install.option=INSTALL_DB_SWONLY \
  UNIX_GROUP_NAME=oinstall \
  INVENTORY_LOCATION=/oradbhome/oraInventory \
  ORACLE_HOME=/oradbhome/dbhome_1 \
  ORACLE_BASE=/oradbhome \
  oracle.install.db.InstallEdition=EE \
  oracle.install.db.OSDBA_GROUP=oinstall \
  DECLINE_SECURITY_UPDATES=true
  
systemctl restart sshd
service sshd restart
export ORACLE_HOME=/u01/app/grid/19c/grid_home
export PATH=$ORACLE_HOME/OPatch:$PATH
export PATH=$ORACLE_HOME/bin:$PATH
export CV_ASSUME_DISTID=OL8
