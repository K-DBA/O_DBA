cat .bash_profile
# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
        . ~/.bashrc
fi

# User specific environment and startup programs

# Oracle Environment Settings
export ORACLE_SID=ORCL
export ORACLE_HOME=/oracle            # Adjust if your actual ORACLE_HOME is different
export TNS_ADMIN=$ORACLE_HOME/network/admin
export PATH=$ORACLE_HOME/bin:$PATH
export LD_LIBRARY_PATH=$ORACLE_HOME/lib

# Optional for ease of troubleshooting
#alias sqlplus='rlwrap sqlplus'
#alias rman='rlwrap rman'

cat .bash_profile
# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
        . ~/.bashrc
fi

# User specific environment and startup programs
# Grid Infrastructure Environment Settings
export ORACLE_SID=+ASM
export ORACLE_HOME=/oragrid             # Replace with actual Grid ORACLE_HOME if different
export TNS_ADMIN=$ORACLE_HOME/network/admin
export PATH=$ORACLE_HOME/bin:$PATH
export LD_LIBRARY_PATH=$ORACLE_HOME/lib

# Optional Aliases for convenience
#alias asmcmd='rlwrap asmcmd'
#alias sqlplus='rlwrap sqlplus'



vi /etc/sysctl.conf

# Oracle recommended kernel parameters for ASM/DB

# Shared memory settings
kernel.shmmax = 2147483648        # Max size of a shared memory segment (2 GB)
kernel.shmall = 524288            # Total shared memory pages (2 GB / 4096)
kernel.shmmni = 4096              # Max number of shared memory segments
kernel.sem = 250 32000 100 256    # Semaphore settings

# File handle settings
fs.file-max = 6815744             # Max number of open file handles
fs.aio-max-nr = 1048576           # Max number of async I/O requests

# Network settings
net.core.rmem_default = 262144
net.core.rmem_max = 16777216      # Max receive buffer
net.core.wmem_default = 262144
net.core.wmem_max = 16777216      # Max send buffer
net.ipv4.conf.all.rp_filter = 2
net.ipv4.conf.default.rp_filter = 2
net.ipv4.ip_local_port_range = 9000 65500

# Memory management
vm.dirty_background_ratio = 10    # % to start writeback
vm.dirty_ratio = 20               # % max dirty pages
vm.swappiness = 10                # Lower = less swapping
kernel.panic_on_oops = 1


export CV_ASSUME_DISTID=OL8
/u01/app/grid/19c/grid_home
./runcluvfy.sh stage -pre crsinst -n red1,red2 -verbose

